{
    "name": "kingch1ll.github.io",
    "version": "1.0.0-beta",
    "description": "The portfolio site for KingCh1ll.",
    "private": true,
    "scripts": {
        "update": "git pull && npm install",
        "checkPacs": "npx npm-check-updates",
        "checkPacs:u": "npx npm-check-updates -u && npm install",
        "test": "eslint . --ignore-path .gitignore",
        "test:fix": "eslint . --ignore-path .gitignore --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "sass:c": "sass assets/scss:assets/css",
        "sass:s": "sass --watch assets/scss:assets/css"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KingCh1ll/Ch1llBlox.git"
    },
    "author": "KingCh1ll",
    "license": "MIT",
    "engines": {
        "node": "16.9.x",
        "npm": "7.x"
    },
    "dependencies": {
        "aos": "^2.3.4",
        "bootstrap": "^5.1.3",
        "sweetalert2": "^11.3.6"
    },
    "devDependencies": {
        "eslint": "^8.4.1",
        "eslint-plugin-import": "^2.25.3",
        "npm": "^8.2.0",
        "prettier": "2.5.1",
        "sass": "^1.44.0"
    }
}
